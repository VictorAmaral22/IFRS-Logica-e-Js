//exc3

var rf,r,num1,num2;
num1=parseInt(prompt("Indique o valor do número um: "));
num2=parseInt(prompt("Indique o valor do número 2: "));
r=0
r=num1+num2;
rf=0;
if(r%2==0){
rf=r+20;
alert(rf)
}
if(r%3==0){
rf=r+30;
alert(rf)
}
if( r%2!=0 && r%3!=0 ){
alert(r)
}

