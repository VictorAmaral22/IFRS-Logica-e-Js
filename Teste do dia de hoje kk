//ex1

var x,y,z;
x=parseInt(prompt("Indique o valor da variável X: "));
y=parseInt(prompt("Indique o valor da variável Y: "));
z=parseInt(prompt("Indique o valor da variável Z: "));
if(x+y>=z){
alert("É um triângulo!")
   if(x==y==z){
   alert("É um triângulo equilátrero")
   }
   if(x==y && x!=z && y!=z || x==z && x!=y && z!=y || y==z && y!=x && z!=x){   
   alert("É um triângulo isósceles!") 
   }
   if(x!=y!=z){
   alert("É um triângulo escaleno!")
}else{
alert("Não é um triângulo!")
}

